{
  "filters": {},
  "rules": {
    "abbr-within-parentheses": true,
    "alex": {
      "allow": ["hooks"]
    },
    "en-capitalization": true,
    "en-spell": {
      "personalDictionary": ["Yuanji", "mkdocs", "textlint", "changelog"]
    },
    "no-todo": true,
    "period-in-list-item": true,
    "terminology": {
      "defaultTerms": true
    },
    "unexpanded-acronym": {
      "min_acronym_len": 3,
      "max_acronym_len": 5,
      "ignore_acronyms": ["YAML"]
    },
    "write-good": true
  }
}
